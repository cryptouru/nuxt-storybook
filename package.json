{
  "name": "nuxt_storybook",
  "version": "1.0.0",
  "description": "Storybook and NuxtJS starter",
  "author": "Cryptouru",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
