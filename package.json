    "babel-preset-vue": "^2.0.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.15.3",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "jest": "^24.5.0",
    "jest-transform-stub": "^2.0.0",
    "node-sass": "^4.11.0",
