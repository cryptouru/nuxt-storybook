{
  "name": "nuxt_storybook_starter",
  "version": "1.0.0",
  "description": "Nuxt.js Storybook starter",
  "author": "Cryptouru",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "build:remote": "npx vue-cli-service build --target wc-async --name foo './components/*.vue' --watch",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "storybook": "start-storybook -p 9001 -c .storybook -s ./static",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.6.3",
    "buefy": "^0.7.2",
    "nuxt": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@storybook/addon-actions": "^4.1.11",
    "@storybook/addon-links": "^4.1.11",
    "@storybook/addon-storyshots": "^4.1.11",
    "@storybook/addons": "^4.1.11",
    "@storybook/vue": "^4.1.11",
    "@vue/cli-service": "^3.5.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.5",
    "cross-env": "^5.0.1",
    "css-loader": "^1.0.1",
    "eslint": "^5.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-vue": "^4.0.0",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "vue-loader": "^15.6.3",
    "vue-template-compiler": "^2.5.22"
  }
}
